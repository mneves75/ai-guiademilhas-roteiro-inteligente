{
  "name": "nextjs-bootstrapped-shipped",
  "version": "0.4.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "E2E_LOCK_DIR=${TMPDIR:-/tmp}/nextjs-next-lock bash ./scripts/with-e2e-lock.sh next build",
    "start": "node scripts/start-standalone.mjs",
    "start:next": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "E2E_LOCK_DIR=${TMPDIR:-/tmp}/nextjs-next-lock bash ./scripts/with-e2e-lock.sh bash ./scripts/type-check.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "bash ./scripts/with-e2e-lock.sh node scripts/test-e2e.mjs",
    "test:e2e:ui": "bash ./scripts/with-e2e-lock.sh playwright test --ui && node scripts/normalize-next-env.mjs",
    "test:e2e:ci": "PW_PROJECT=chromium bash ./scripts/with-e2e-lock.sh node scripts/test-e2e.mjs",
    "security:asvs-check": "bash ./scripts/asvs-check.sh",
    "security:audit": "bash ./scripts/security-audit.sh",
    "verify": "pnpm lint && pnpm type-check && pnpm test && pnpm build && pnpm db:smoke && pnpm test:e2e:ci",
    "verify:ci": "pnpm lint && pnpm type-check && pnpm test && pnpm build && pnpm db:schema-parity && pnpm db:smoke:sqlite && pnpm test:e2e:ci",
    "readiness": "node scripts/readiness-check.mjs",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:generate:pg": "drizzle-kit generate --config=drizzle.config.postgres.ts",
    "db:generate:sqlite": "drizzle-kit generate --config=drizzle.config.sqlite.ts",
    "db:generate:d1": "drizzle-kit generate --config=drizzle.config.d1.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:d1": "drizzle-kit migrate --config=drizzle.config.d1.ts",
    "db:push": "drizzle-kit push",
    "db:push:pg": "drizzle-kit push --config=drizzle.config.postgres.ts",
    "db:push:sqlite": "drizzle-kit push --config=drizzle.config.sqlite.ts",
    "db:push:d1": "drizzle-kit push --config=drizzle.config.d1.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "NODE_OPTIONS=--conditions=react-server tsx ./src/db/seed.ts",
    "db:assert-seed": "NODE_OPTIONS=--conditions=react-server tsx ./src/db/assert-seed.ts",
    "db:portability-check": "NODE_OPTIONS=--conditions=react-server tsx ./src/db/portability-check.ts",
    "db:schema-parity": "NODE_OPTIONS=--conditions=react-server tsx ./src/db/schema-parity.ts",
    "db:smoke:pg:local": "bash ./scripts/db-smoke-postgres-local.sh",
    "db:smoke:sqlite": "bash ./scripts/db-smoke-sqlite.sh",
    "db:smoke": "bash ./scripts/db-smoke.sh",
    "db:reset": "drizzle-kit push --force && NODE_OPTIONS=--conditions=react-server tsx ./src/db/seed.ts",
    "framework:bootstrap": "bash ./scripts/framework-upstream.sh bootstrap",
    "framework:status": "bash ./scripts/framework-upstream.sh status",
    "framework:preview": "bash ./scripts/framework-upstream.sh preview",
    "framework:check": "bash ./scripts/framework-upstream.sh check",
    "framework:doctor": "bash ./scripts/framework-upstream.sh doctor",
    "framework:sync": "bash ./scripts/framework-upstream.sh sync",
    "framework:sync:verify": "bash ./scripts/framework-upstream.sh sync --verify"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.24",
    "@aws-sdk/client-s3": "^3.987.0",
    "@growthbook/growthbook": "^1.6.4",
    "@growthbook/growthbook-react": "^1.6.4",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@neondatabase/serverless": "^1.0.2",
    "@next/mdx": "^16.1.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^1.0.7",
    "@react-email/render": "^2.0.4",
    "@sentry/node": "^10.38.0",
    "@stripe/stripe-js": "^8.7.0",
    "@vercel/blob": "^2.2.0",
    "ai": "^6.0.79",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "geist": "^1.7.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-mdx-remote": "^6.0.0",
    "next-themes": "^0.4.6",
    "pino": "^10.3.1",
    "postgres": "^3.4.8",
    "posthog-js": "^1.345.3",
    "posthog-node": "^5.24.15",
    "prom-client": "^15.1.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-email": "^5.2.8",
    "reading-time": "^1.5.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.9.2",
    "server-only": "^0.0.1",
    "shiki": "^3.22.0",
    "stripe": "^20.3.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "optionalDependencies": {
    "better-sqlite3": "^12.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@next/eslint-plugin-next": "^16.1.6",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.3.9",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "drizzle-kit": "^0.31.9",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "@esbuild-kit/core-utils>esbuild": "0.25.12"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
